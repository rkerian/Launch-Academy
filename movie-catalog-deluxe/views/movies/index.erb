<!doctype html>
<html>
  <head>
    <title>Movies</title>
  </head>

  <body>
    <h2>Movies</h2>
    <% @movies.each do |movie| %>
      <a href="/movies/<%= movie["id"] %>"><%= movie["title"] %></a><%=
        "Year: #{movie["year"]} Rating: #{movie["rating"]} Genre: #{movie["genre"]} Studio: #{movie["studio"]}" %>
      <br>
    <% end %>
  </body>
</html>
